@model UploadTalkScheduleFile
@{
    ViewData["Title"] = "Generate Talk Schedule";
}

<form method="post"  action="/MeetingManagement/UploadTalkScheduleFile" enctype="multipart/form-data">
    <div class="row">
        <h4 class="alert alert-info">Please select the file to upload</h4>
        <div class="col-md-6">
            <div class="form-group">
                <label class="control-label">
                    Upload Talk Schedule File
                </label>
                <input type="file" required="required"  asp-for="TalkScheduleFile" class="form-control" />
            </div>
        </div>

    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-primary" value="Generate Schedule" />
    </div>
</form>
